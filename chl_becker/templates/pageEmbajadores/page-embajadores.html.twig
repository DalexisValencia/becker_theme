<head><title>Emabajdores</title></head>
<section class="relative mb-12 lg:mb-0">
  {% for item in embajadores %}
    <div class="flex h-screen items-center" id="btn-hero-video">
      <picture>
        <source media="(max-width:768px)" srcset="{{ item.img_mobile }}"
                alt="Embajadores becker">
        <img
          class="absolute h-full inset-0 md:object-center mt-12 object-cover object-right p-big-promo__img-background top-0 w-full"
          src="{{ item.img_desktop }}" alt="Embajadores becker">
      </picture>
      <div class="flex text-white mt-64">
        <div class="lg:w-1/2 md:w-2/3">
          <h1 data-aos="fade-right" class=" leading-tight lg:ml-16 ml-8 text-4xl">{{ item.title }}</h1>
          <p class="font-third lg:ml-16 ml-8 py-4 relative text-base w-4/5">{{ item.description }}</p>
            <a data-element="cta-embajadores-banner" data-animation-in="fadeInUp" href="{{ not logged_in ? item.button_url : '/embajadores/perfil' }} "
               class="bg-secondary c-button flex items-center justify-between my-3 px-3 py-2 text-xs uppercase lg:w-40 lg:mb-16 w-48 lg:ml-16 ml-8 relative">{{ item.button_title }}
              <span>
								<svg width="18" height="19" viewbox="0 0 18 19" fill="none" xmlns="http://www.w3.org/2000/svg">
									<path d="M3.6 0V1.9H14.931L0 17.6605L1.269 19L16.2 3.2395V15.2H18V0H3.6Z" fill="white"/>
								</svg>
							</span>
            </a>
        </div>
        
        {% if videoAmbassador %}  
        <div class="hidden lg:block container-video-top absolute">
          {% block videoAmbassador %}
            {% set nickname = item.nickname %}
            {% set title = item.title_challenge %}
            {% set hashtag = item.hashtag %}
            {% set linkVideo = item.video_url %}
            {% include '@chl_becker/components/video-ambassadors.html.twig' %}
          {% endblock %}
        </div>
        {% endif %}

        <button class="play-video-ambass btn-modal duration-150 ease-linear focus:outline-none  outline-none hidden"
                data-video-src="{{ item.video_url }}" type="button">
          <svg class="-mt-64 absolute circle-video left-0 lg:-mt-28 m-auto md:-mt-64 right-0" width="107" height="107"
               viewbox="0 0 107 107" fill="none" xmlns="http://www.w3.org/2000/svg">
            <circle cx="53.5" cy="53.5" r="53.5" fill="white"/>
            <path d="M72 53.5L44.25 69.5215L44.25 37.4785L72 53.5Z" fill="black"/>
          </svg>
        </button>
      </div>
    </div>
    <div id="hero-video" class="absolute top-0 w-full"></div>
  {% endfor %}
</section>
<section class="relative bg-white overflow-hidden">
  <div class="c-competitor lg:m-4">
    <h2 class="text-xl">Participantes</h2>
    <div data-element="c-user-challenges" class="grid grid-cols-1 justify-items-center lg:grid-cols-3 mb-4 "></div>
    <p id="show-more-videos" data-element="b-show-more-user-challenges" class="show-more text-center cursor-pointer">Cargar m√°s</p>
  </div>
</section>

<div id="modal-id-ambass"
     class="hidden overflow-x-hidden overflow-y-auto fixed inset-0 z-50 outline-none focus:outline-none justify-center items-center">
  <div
    class="relative w-full my-6 mx-auto max-w-3xl">
    <!--content-->
    <div
      class="relative flex flex-col w-full outline-none focus:outline-none">
      <!--header-->
      <div class="flex items-start justify-between">
        <button
          class="close-modal bg-secondary border-0 float-right focus:outline-none font-semibold leading-none ml-auto outline-none rounded-full text-white">
					<span class="block flex focus:outline-none h-8 items-center justify-center outline-none w-8">
						
					</span>
        </button>
      </div>
      <!--body-->
      <div class="relative px-6 flex-auto">
        <iframe width="100%" height="600" src="" title="YouTube video player" frameborder="0"
                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                allowfullscreen></iframe>
      </div>
    </div>
  </div>
</div>
<div class="close-modal hidden opacity-75 fixed inset-0 z-40 bg-black" id="modal-id-ambass-backdrop"></div>
